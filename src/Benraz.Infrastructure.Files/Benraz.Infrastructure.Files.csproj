<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Benraz.Infrastructure.Files</PackageId>
    <Description>File management package. Provides services for working with files on different platforms.</Description>
    <Company>Benraz</Company>
    <ProjectGuid>951C5C83-2196-4B2E-9091-16650975DECF</ProjectGuid>
    <Authors>Benraz</Authors>
    <License>MIT License</License>
    <Copyright>Copyright Â©</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl></RepositoryUrl>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageIconUrl></PackageIconUrl>
    <IsPackable>true</IsPackable>
    <Version>1.0.3</Version>
    <AssemblyVersion>1.0.3</AssemblyVersion>
    <FileVersion>1.0.3</FileVersion>
    <AssemblyName>Benraz.Infrastructure.Files</AssemblyName>
    <RootNamespace>Benraz.Infrastructure.Files</RootNamespace>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(MSBuildThisFileName).xml</DocumentationFile>

	<PackageOutputPath>$(SolutionDir)BenrazNugetPackages</PackageOutputPath>
	<Title>Benraz.Infrastructure.Files</Title>

	<IncludeSymbols>true</IncludeSymbols>
	<SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Blobs" Version="12.21.2" />
    <PackageReference Include="FluentFTP" Version="51.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Benraz.Infrastructure.Common\Benraz.Infrastructure.Common.csproj" />
  </ItemGroup>

</Project>



